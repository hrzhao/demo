<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping
        package="hrzhao.beans"
        >

    <class name="ProcessBean" table="process">

        <id name="id">
            <generator class="native"/>
        </id>
        <property name="nextId" column="nextId" type="integer"/>
        <property name="negativeId" column="negativeId" type="integer"/>
        <property name="tips" column="tips" type="string"/>
        <property name="useTips" column="useTips" type="boolean"/>
        <property name="className" column="className" type="string"/>
        <property name="param" column="param" type="string"/>
    </class>

</hibernate-mapping>
